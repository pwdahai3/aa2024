.matchtips span{ display: inline-block; margin-right: 12px; vertical-align: middle; }
.matchtips span i{ display: inline-block; width: 10px; height: 10px; -webkit-border-radius: 50%;border-radius: 50%; margin-right: 2px; vertical-align: middle; background: #ffffff; border-width:3px; border-style: solid; }
.matchtips span i.i1{ border-color:#39a8ef;}
