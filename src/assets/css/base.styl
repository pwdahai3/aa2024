//运行中...图标明暗动画
.shadow-icon-color-success
  color: #333333;
  animation:mystatus 1s infinite;
  -webkit-animation:mystatus 1s infinite;
@keyframes mystatus
{
  from {color:#333333}
  to {color:#67c23a}
}
@-webkit-keyframes mystatus /*Safari and Chrome*/
{
  from {color:#333333}
  to {color:#67c23a}
}
