// 提示语
.tips{font-size:12px;color:#606266;margin-bottom: 20px;line-height:20px;}//margin-left: 15px;
// 文字溢出
.txtOverflow{
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
//面包屑导航
.breadcrumb{
  font-weight:800;
  background-color:#f7f7f7;
  line-height: 30px;
  font-size: 14px;
  margin-bottom: 10px;
  padding-left: 20px;
  box-sizing: border-box;
}
// 表格横向滚动
.scroll-table{
  overflow-x:auto;
}

//图表背景颜色
.chart-content{
  background-color:#f7f7f7;
  position relative;
  // box-shadow: 2px 2px 5px #ccc;
}

//表格背景颜色
.table-bg{
  background: #fff;
}

//图表展示 右侧操作列表
.operationBtnUl {
  position: fixed;
  right: 15px;
  bottom: 50px;
}
.operationBtnUl li {
  background: #eee;
  padding: 10px;
}
.operationBtnUl li img {
  cursor: pointer;
}

//带边框div的压边框标题
.titleSpan{
  position: absolute;
  top: -10px;
  left: 13px;
  background: #fff;
  padding: 0 10px;
  font-size: 16px;
}

//文字禁止选中
.userSelectNo{
  -moz-user-select:none; /*火狐*/
  -webkit-user-select:none; /*webkit浏览器*/
  -ms-user-select:none; /*IE10*/
  -khtml-user-select:none; /*早期浏览器*/
  user-select:none;
}

